<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<elements>
<LoginPageObjects>
	<username locator="id" value="txtUsername"/>
	<password locator="id" value="txtPassword"/>
	<loginButton locator="id" value="btnLogin"/>
</LoginPageObjects>


<HomePageObjects>
	<welcomeButton locator="xpath"	value="//A[@id='welcome']"/>
